{
  "name": "vue-cli-3-nuxt-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts",
    "generate": "nuxt-ts generate",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "nuxt-ts | vue-cli-service test:e2e --url http://localhost:3000"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.3.8",
    "nuxt": "^2.11.0",
    "register-service-worker": "^1.6.2",
    "ts-node": "^8.6.2",
    "vue-property-decorator": "^8.0.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.6",
    "@types/jest": "^24.9.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-e2e-cypress": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.0",
    "@vue/test-utils": "^1.0.0-beta.30",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "typescript": "^3.7.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
